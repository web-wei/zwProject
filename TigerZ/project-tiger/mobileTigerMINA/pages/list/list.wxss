/* list.wxss */
.list_content {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  overflow: scroll;
}

.list_content .list_header {
  width: 100%;
  height: 92rpx;
  background-color: #fdc012;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  display: flex;
  align-items: center;
  justify-content: center;
}
.list_header_allCity{
  width: 25%;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}
.list_header_allCity span{
  width: 21rpx;
  height: 28rpx;
  background: url(https://m.tigerz.nz/img/nav-dingwei@2x.png) no-repeat;
  background-size: contain;
  margin-right: 8rpx;
}
.list_header_allCity view{
  /*width: 88%;*/
  font-size: 28rpx;
  font-weight: bold;
  color: #fff;
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.list_header_broker{
  width: 12%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  /*background-color: #ececec;
  border: 0;
  outline: none;
  background: url(https://m.tigerz.nz/img/xiaoxi@2x.png) center no-repeat;
  background-size: 37rpx 38rpx;*/
}
.list_header_search{
  padding-left: 98rpx;
  width: 63%;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  font-size: 27rpx;
  color: #fff;
  background: #b3890c url(https://m.tigerz.nz/img/nav-sousuo@2x.png) no-repeat;
  background-size: 30rpx;
  background-position: 37rpx 15rpx;
}
.list_content .list_main {
  width: 100%;
  height: 100%;
  background-color: #fff
}
.list_content .list_main .list_main_filtrate {
  border-bottom: 1px solid #e0e0e0;  
  width: 100%;
  height: 81rpx;
  background-color: #f9f9f9;
  position: fixed;
  top: 92rpx;
  left: 0;
  z-index: 999
}

.list_content .list_main .list_main_filtrate>ul {
  width: 100%;
  height: 81rpx;
}

.list_content .list_main .list_main_filtrate>ul li {
  width: 24%;
  height: 81rpx;
  float: left;
  font-family: pingfang;
  font-size: 28rpx;
  line-height: 81rpx;
  padding: 36rpx 0 0 125rpx;
  position: relative
}

.list_content .list_main .list_main_filtrate>ul li p {
  width: 100%;
  height: 100%
}

.list_content .list_main .list_main_filtrate>ul li p span:first-of-type {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0
}

.list_content .list_main .list_main_filtrate>ul li p .list_main_triangle {
  float: left;
  width: 0;
  height: 0;
  border: 11rpx solid transparent;
  border-top-color: #656565
}

.list_content .list_main .list_main_filtrate>ul li p .list_filtrate_ball {
  float: right;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background-color: #fd6e05;
  margin-right: 10rpx
}

.list_content .list_main .list_main_filtrate>ul li:first-of-type p span:first-of-type {
  padding-left: 50rpx
}

.list_content .list_main .list_main_filtrate>ul li:nth-of-type(2) p span:first-of-type {
  padding-left: 18rpx
}

.list_content .list_main .list_main_filtrate>ul li:nth-of-type(3) p span:first-of-type {
  padding-left: 0;
  width: 72%;
  text-align: center
}

.list_content .list_main .list_main_filtrate>ul li:nth-of-type(3) {
  width: 28%;
  padding-left: 155rpx
}

.list_content .list_main .list_main_filtrate>ul li:last-of-type p span:first-of-type {
  padding-left: 42rpx
}

.list_content .list_main .list_common_color {
  color: #fbc730 !important
}


.list_common_color1{
  color: #fbc730 !important
}
.list_common_color2{
  color: #323a3d !important
}
.list_common_color3:first-of-type{
  color: #fbc730 !important
}

.list_common_color5{
  color: #fbc730 !important
}
.list_common_color4{
  color: #323a3d !important
}

.list_content .list_main .list_common_color_border {
  border-top-color: #fbc730 !important
}

.list_content .list_main .list_filtrate_area, .list_content .list_main .list_filtrate_campus {
  font-size: 25rpx;
  font-weight: 500;
  position: fixed;
  background-color: #fff;
  top: 173rpx;
  left: 0;
  bottom: 0;
  width: 100%;
  overflow: scroll;
  z-index: 999
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaLeft, .list_content .list_main .list_filtrate_campus .list_filtrate_areaLeft {
  float: left;
  width: 50%;
  height: 100%;
  line-height: 82rpx;
  background-color: #f6f6f6;
  overflow: scroll;
  font-family: Arial;
  font-size: 28rpx;
  color: #323a3d
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaLeft li, .list_content .list_main .list_filtrate_campus .list_filtrate_areaLeft li {
  height: 82rpx;
  padding-left: 24rpx;
  border-bottom: 1rpx solid #f6f6f6
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaLeft .list_areaLeft_style, .list_content .list_main .list_filtrate_campus .list_filtrate_areaLeft .list_areaLeft_style {
  background-color: #fff
}

.listFiltrateAreaLeftbackgroundColor1:first-of-type{
  background-color: #fff;
}

.listFiltrateAreaLeftbackgroundColor{
  background-color: #fff;
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaRight, .list_content .list_main .list_filtrate_campus .list_filtrate_areaRight {
  float: right;
  width: 50%;
  height: 100%;
  overflow: scroll;
  background-color: #fff
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaRight ul, .list_content .list_main .list_filtrate_campus .list_filtrate_areaRight ul {
  padding-left: 28rpx
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaRight ul li, .list_content .list_main .list_filtrate_campus .list_filtrate_areaRight ul li {
  font-family: Arial;
  font-size: 28rpx;
  color: #323a3d;
  text-indent: 13rpx;
  height: 82rpx;
  line-height: 82rpx;
  border-bottom: 1rpx solid #e0e0e0;
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaRight ul li label:first-of-type, .list_content .list_main .list_filtrate_campus .list_filtrate_areaRight ul li label:first-of-type {
  font-family: Arial;
  font-size: 28rpx;
  color: #323a3d;
  float: left;
  width: 95%
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaRight ul li label:last-of-type, .list_content .list_main .list_filtrate_campus .list_filtrate_areaRight ul li label:last-of-type {
  position: absolute;
  top: 24rpx;
  right: 10rpx;
  width: 31rpx;
  height: 31rpx;
  background: #fff;
  border: 1rpx solid #d9d9d9
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaRight ul li .duigou:after, .list_content .list_main .list_filtrate_campus .list_filtrate_areaRight ul li .duigou:after {
  position: absolute;
  left: 8rpx;
  top: 1rpx;
  content: '';
  display: block;
  width: 8rpx;
  height: 15rpx;
  border-right: 2rpx solid #fbc730;
  border-bottom: 2rpx solid #fbc730;
  transform: rotate(45deg)
}

.list_content .list_main .list_filtrate_area .list_filtrate_areaRight ul li checkbox, .list_content .list_main .list_filtrate_campus .list_filtrate_areaRight ul li checkbox {
  visibility: hidden;
  float: right;
  width: 5%;
  height: 40%;
  margin-top: 23rpx;
  border: 1rpx solid #e3e3e3
}

.list_content .list_main .list_filtrate_roomNum {
  font-size: 28rpx;
  font-weight: 500;
  position: fixed;
  background-color: #fff;
  top: 173rpx;
  left: 0;
  bottom: 0;
  width: 100%;
  overflow: scroll;
  z-index: 999
}

.list_filtrate_roomNum checkbox-group{
  padding: 0 21rpx;
}

.list_filtrate_roomNum checkbox-group label{
  display: block;
  font-family: Arial;
  font-size: 28rpx;
  color: #323a3d;
  text-indent: 13rpx;
  height: 82rpx;
  line-height: 82rpx;
  border-bottom: 1px solid #e0e0e0;
}

.list_filtrate_roomNum checkbox-group label checkbox{
  float: right;
}

.list_content .list_main .list_filtrate_roomNum .list_roomNum_confirm {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 132rpx;
  padding: 29rpx 33rpx;
  border-top: 1rpx solid #e0e0e0;
  background-color: #fff;
  color: #fff
}

.list_content .list_main .list_filtrate_roomNum .list_roomNum_confirm view {
  width: 100%;
  height: 100%;
  background-color: #f05d2f;
  border-radius: 6rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center
}

.list_content .list_main .list_filtrate_sort {
  padding-left: 20rpx;
  font-size: 28rpx;
  font-weight: 500;
  position: fixed;
  background-color: #fff;
  top: 173rpx;
  left: 0;
  bottom: 0;
  width: 100%;
  overflow: scroll;
  z-index: 999
}

.list_content .list_main .list_filtrate_sort li {
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #f5f5f5
}

.list_content .list_main .list_filtrate_sort li .list_filtrate_tick {
  width: 23rpx;
  height: 30rpx;
  border-right: 2rpx solid #fbc730;
  border-bottom: 2rpx solid #fbc730;
  transform: rotate(45deg);
  float: right;
  margin: 20rpx 45rpx 0 0;
}

.list_content .list_main .list_main_items {
  height: 1037rpx;
  display: block;
  padding-top: 173rpx;
  background-color: #fff;
  white-space: nowrap;
  overflow: hidden;
}

.list_content .list_main .list_main_items .noHouseRed {
  height: 120rpx;
  font-size: 30rpx;
  line-height: 120rpx;
  text-align: center;
  color: red
}

.list_content .list_main .list_main_items .list_item {
  height: 260rpx;
  border-bottom: 1px solid #e0e0e0;
  position: relative;
  padding: 32rpx
}

.list_content .list_main .list_main_items .list_item navigator {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 10
}

.list_content .list_main .list_main_items .list_item image {
  width: 34%;
  height: 189rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto 0;
  background: url(https://m.tigerz.nz/img/lazyLoading.jpg) center no-repeat;
  background-size: contain;
}

.list_content .list_main .list_main_items .list_item .list_item_describe {
  float: right;
  width: 60%;
  height: 100%
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_text {
  display: -webkit-box;
  white-space:pre-line;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  font-size: 32rpx;
  margin-top: 3rpx;
  line-height: 38rpx;
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_price {
  font-family: LanTingHeiJian;
  font-size: 30rpx;
  font-weight: bold;
  color: #fb5640;
  line-height: 45rpx
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_site {
  font-family: Arial;
  font-size: 24rpx;
  color: #939393;
  line-height: 36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_gather {
  display: flex;
  align-items: center;
  height: 40rpx;
  font-size: 23rpx;
  color: #8b8b8b
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_gather span {
  width: 25rpx;
  height: 25rpx;
  line-height: 25rpx;
  margin-right: 10rpx
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_gather .list_item_bed {
  background: url(https://m.tigerz.nz/img/bed@3x.png) no-repeat;
  background-size: contain;
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_gather .list_item_bath {
  background: url(https://m.tigerz.nz/img/bath@3x.png) no-repeat;
  background-size: contain;
  margin-left: 25rpx;
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_gather .list_item_date {
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.list_content .list_main .list_main_items .list_item .list_item_describe .list_item_gather .list_item_date view {
  float: left;
  font-size: 23rpx;
  color: #939393;
  line-height: 40rpx;
}

.list_content .list_main .more_item .more_item_top {
  margin: 0 auto;
  width: 92%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background-color: #f9f9f9;
  color: #849aad;
  font-size: 30rpx;
  border-radius: 4rpx
}
.auction_mask {
  position: fixed;
  top: 173rpx;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 20;
  background: rgba(255, 255, 255, 0.7)
}

.auction_mask image {
  width: 120rpx;
  height: 120rpx;
  position: absolute;
  top: 420rpx;
  left: 50%;
  margin-left: -60rpx
}
